{
  "extends": [ "standard" ],
  "plugins": [ "babel", "flowtype" ],
  "parser": "babel-eslint",
  "rules": {
    "block-spacing": [0],
    "comma-style": [1, "first"],
    "key-spacing": [0],
    "no-multi-spaces": [0],
    "object-curly-spacing": [1, "always"],
    "one-var": [0],
    "arrow-parens": ["warn", "always"]
  }
}
